page-course-list {
    .scroll-content{
        background: #ffffff url(../assets/imgs/home-new/main-bg.png) no-repeat center -15px;
        border-top: none !important;
    }
   .course_title{
       display: flex;
       justify-content: center;
       align-items: center;
       flex-direction: row;
       height: 35px;
       line-height: 35px;
       width: 100%;
       position: relative;
       margin-top: 37px;
       padding-bottom: 10px;
       border-bottom: 1px solid #ebebeb;
   } 
   .course_bottomLine{
       position: absolute;
       width:calc(100vw / 4);
       background: url('../assets/imgs/course/courseBorser.png') no-repeat center;
       background-size: 19px 3px;
       height: 3px;
       bottom: 0;
       left: 0;
   }
   .course_title_list{
       flex: 1;
       text-align: center; 
       font-size: 13px;
       color: #999;
       display: flex;
       justify-content: center;
       align-items: center;
       position: relative;
   }
   .course_title_list.cover{
       background: url('../assets/imgs/course/course_title_bg.png') no-repeat 20px bottom;
       background-size:30px auto;
   }

   .course_title_li{
      font-size: 15px; 
      color: #333;
      font-weight: bold;
      background: url('../assets/imgs/course/course_title_list.cur.png') no-repeat left center;
      background-size:13px auto;
      padding-left: 18px;
   }
   .course_pad{
       overflow-x: hidden;
       overflow-y: auto;
   }
   .course_one_pad{
       display: flex;
       justify-content: flex-start;
       align-items: center;
       flex-wrap: wrap;
       padding: 18px 15px 0 0;
   }
   .course_one_list{
       padding-bottom: 18px;
       width: 50%;
   }
   .course_one_frame{
       display: flex;
       justify-content: flex-start;
       align-items: flex-start;
       padding-left: 15px;
       flex-direction: column;
   }
   .course_one_img img{
       width: 100%;
       height: 100%;
       vertical-align: top;
       
   }
   .course_one_name{
       font-size: 16px;
       color: #333;
       font-weight: 600;
       margin-bottom: 7px;
       overflow: hidden;
       text-overflow:ellipsis;
       display: -webkit-box;
       -webkit-line-clamp: 1;
       -webkit-box-orient: vertical;
   }
   .course_one_read{
       color: #999999;
       font-size: 12px;
   }
   .course_one_img{
      width: 100%; 
      height: 121px;
      margin-bottom: 7px;
      box-shadow:0 2px 3px #ececec;
      border-radius: 10px;
      overflow: hidden;
      position: relative;
      background: #ffffff;
   }
   .course_one_redPad{
       position: absolute;
       left: 0px;
       top: 0px;
       height: 16px;
       line-height: 16px;
       padding: 0 10px;
       color: #fff;
       font-size: 12px;
       background-color: #fe4743;
       border-bottom-right-radius: 10px;
   }
   .course_img{
       width: 100%;
   }
   .course_box_all{
       padding: 26px 15px 0 15px;
       display: flex;
       justify-content: center;
       align-items: center;
       flex-direction: column;
   }
   .course_box_title{
       width: 100%;
       display: flex;
       justify-content: flex-start;
       align-items: center;
   }
   .course_box_title_role{
       height: 19px;
       width: 19px;
       border-radius: 50%;
       background-color: #697ff0;
       display: flex;
       justify-content: center;
       align-items: center;
       color: #fff;
       font-size: 14px;
       
   }
   .course_box_title_txt{
       font-size: 16px;
       color: #333;
       flex: 1;
       padding-left: 10px;
       text-align: left;
       font-weight: 700;
   }
   .course_box_title_{
       padding:26px 0 10px 0;
       color: #333;
       font-size: 13px;
       text-align: left;
       width: 100%;
   }
   .course_box_slide{
       padding-right: 15px;
       min-width: 80%;
   }
   .course_box_slide img{
       width: 100%;
       vertical-align: top;
      
   }
   .course_all_slide{
       max-width: 100%;
       overflow-x: auto;
       overflow-y: hidden;
       display: flex;
       justify-content: flex-start;
       align-items: flex-start;
   }
  .course_box_cont{
      background-color: #fff;
      box-sizing: border-box;
  }
  .course_box_number{
      padding: 11px 0 28px 0;
      display: flex;
      justify-content: center;
      width: 100%;
      align-items: center;
      color: #888888;
      font-size: 13px;
  }
  .course_BSTL_footer{
      overflow: hidden;
      position: relative;
      padding:0 15px 50px 15px;
  }
  .course_BSTL_footer img{
      vertical-align: top;
      width: 100%;
  }
  .course_BSTL_footer_title{
      position: absolute;
      width: 100%;
      box-sizing: border-box;
      top: -3px;
      left: 15px;
      color: #333;
      font-size: 14px;
      text-align: left;
  }
  .course_call_pad{
      position: fixed;
      bottom: 60px;
      right: 20px;
      width: 60px;
      height: 60px;
      border-radius: 50%;
      background-color: #fecf02;
      display: flex;
      justify-content: center;
      flex-direction: column;
      align-items: center;
      box-shadow: 0 3px 12px #e0e0e0;
      z-index: 150;
  }
  .course_call_pad div{
      color: #fff;
      font-size: 16px; 
  }
  .course_steam_all{
    padding: 26px 0 26px 15px;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-pack: center;
    -webkit-justify-content: center;
    -ms-flex-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    -webkit-align-items: center;
    -ms-flex-align: center;
    align-items: center;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    -webkit-flex-direction: column;
    -ms-flex-direction: column;
    flex-direction: column;
    border-top: 5px solid #f6f6f6;
    border-bottom: 5px solid #f6f6f6;
  }
  .course_steam_number{
    padding: 11px 0 26px 0;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-pack: center;
    -webkit-justify-content: center;
    -ms-flex-pack: center;
    justify-content: center;
    width: 100%;
    -webkit-box-align: center;
    -webkit-align-items: center;
    -ms-flex-align: center;
    align-items: center;
    color: #888888;
    font-size: 13px;
  }
  .course_steam_title_role{
    height: 19px;
    width: 19px;
    border-radius: 50%;
    background-color: #feedde;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-pack: center;
    -webkit-justify-content: center;
    -ms-flex-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    -webkit-align-items: center;
    -ms-flex-align: center;
    align-items: center;
    color: #fca75a;
    font-size: 14px;
  }
  .course_steam_title{
    width: 100%;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-pack: start;
    -webkit-justify-content: flex-start;
    -ms-flex-pack: start;
    justify-content: flex-start;
    -webkit-box-align: center;
    -webkit-align-items: center;
    -ms-flex-align: center;
    align-items: center;  
    padding-bottom: 26px;
  }
  .course_steam_all:last-child{
      border-bottom: none;
  }
  .course_box_serve{
      padding-bottom: 20px;
  }
   //修改原生
   .swiper-slide{
       align-items: flex-start;
   }
   .swiper-slide img{
       vertical-align: top;
   }
   .p-nomore{
    text-align: center;
    padding-bottom: 20px;
    color: #999999;
    margin: 0;
    font-size: 12px;
    width: 100%;
}
.no_data_show{
    padding-top: 85px;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
    width: 100%;
}
.no_data_img{
    background:url('../assets/imgs/teacher/no_data_img.png') no-repeat top center;
    background-size:175px 134px;
    width: 175px;
    height: 134px;
}
.no_data_txt{
    padding-top: 32px;
    font-size: 16px;
    color: #999999;
    width: 100%;
    text-align: center;
}
}
