//页面初始化 start
p{
    padding: 0;
    margin: 0;
}
.scroll-content{
    overflow: hidden;
}
//页面初始化 end



.read_content{
//  width: 100%;
//  height: 100%;
    // background: #f3f3f3;
}

.read_process,.update_item{
    display: flex;
    align-items: center;
    padding-bottom: 15px;
    
}
.book_cover,.b_cover{
    width: 120px;
    height: 100px;
    background: rgba(0,0,0,0.05);
    box-shadow: 0 1px 5px rgba(0, 0, 0, 0.16);
    border-radius: 5px;
    overflow: hidden;
}
.book_cover img,.b_cover img{
    width: 120px;
    height: 100px;
    border-radius: 4px;
}
.book_detail,.b_detail{
    flex: 1;
    margin-left: 18px;
}
.book_process{
    width: 70%;
    height: 9px;
//  background: #999999;
    border-radius: 6px;
    position: relative;
    margin-top: 30px;
    border:1px solid #57a732;
    display: flex;
    align-items: center;
    justify-content: flex-start;
    padding: 0 2px;
}
.percentage{
    position: absolute;
}
.b_pro{
    width: 50px;
    height:5px;
    border-radius: 5px;
    background: #57a732;
}
.percentage{
    right: -35px;
    top:-3px;
    color: #4fb233;
    font-size: 12px;
}
.difficulty{
    display: inline-block;
    width: 10px;
    height: 10px;
    margin-left: 6px;
//  border: 1px solid #999999;
}


.read_title{
    font-size: 16px;
    color: #333333;
    padding: 15px 0;
}
.read_line{
    display: inline-block;
    height: 16px;
    width: 3px;
    background: #d81d4a;
    margin-right: 15px;
    margin-bottom: -2px;
}
.update_ing,.last_read{
    background: #ffffff;
    padding:  0 12px;
}
.last_read{
    border-bottom: solid 4px #f5f5f5;
}
.book_name{
    font-size: 16px;
    color: #251b1b;
    margin-bottom:7px;
}
.nan_du{
    color: #251b1b;
    font-size: 14px;
    margin-bottom:7px;
}
.update_to{
    color: #333333;
    font-size: 12px;
}
.ever_day{
   color: #cb2247; 
}

//阅读  new  start 
.read_content_new{
    display: flex;
    flex-direction: column;
    width: 100%;
    height: 100%;
}
.read_list{
    overflow: auto;
    padding-top: 20px;
    position: relative;
}

.book_base{
    flex: 1;
    overflow: auto;
    position: relative;
//  padding: 0 15px;
}
.read_item{
    display: flex;
    height: 115px;
    align-items: center;
    margin: 0 13px;
    margin-bottom: 15px;
    background: #ffffff;
    border-radius: 10px;
    box-shadow: 0 0 13px  rgba(209,212,218,0.55);
}
.read_item_con{
    display: flex;
    align-items: center;
    width: 100%;
    height: 100%;
    padding: 0 13px;
}
.read_item_sliding{
    height: 115px;
    background: #ffffff;
    border-radius: 10px;
    box-shadow: 0 0 13px  rgba(209,212,218,0.55);
    margin-bottom: 15px;
}
.read_item_bottom{
    margin-bottom: 15px;
}
.book_img{
    width: 72px;
    height: 98px;
    position: relative;
    overflow: hidden;
    box-shadow:0 2px 3px #d7d7d7;
    border-radius: 10px;
}
.read_book_icon{
    width: 72px;
    height: 98px;
    border-radius: 8px;
}
.is_cost{
  position: absolute;
  left: 0px;
  top:0px;
  color: #ffffff;
  font-size: 10px;
  background: #fe4743;
  height: 14px;
  min-width: 50px;
  display: flex;
  align-items: center;
  justify-content: center;
//transform: rotate(-45deg);
  border-bottom-right-radius: 10px;
  border-top-left-radius: 10px;
  padding: 3px 10px;
}
.drap_hot_zone{
    height: 50px;
    display: flex;
    align-items: center;
    justify-content: center;
    opacity: 0;
    pointer-events: none;
    position: absolute;
//  background: bisque;
}
.drap_hot_zone_show{
    position: relative;
 opacity: 1;
 pointer-events: auto;   
}
.drap_hot_zone_1{
    height: 20px;
}
.slid_icon{
    width: 15px;
    height: 8px;
}
.slid_icon_trans{
    transform: rotate(180deg);
    transition: all .3s;
}
.allContent{
    width: 100%;
    height: 100%;
//  padding: 0 15px;
//  overflow: hidden;
}

