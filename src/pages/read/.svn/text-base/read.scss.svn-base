.books_name{
    font-size: 16px;
    color: #251b1b;
    width: 100%;
    word-break:break-all;
    word-wrap:break-word;
//  white-space: pre-wrap;
//  width: 190px;
    overflow: hidden;
    text-overflow:ellipsis;//文本溢出显示省略号
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}
